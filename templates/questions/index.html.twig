{% extends 'base.html.twig' %}

{% block title %}
    {{ 'questions.title'|trans }}
{% endblock %}

{% block body %}
    <div class="p-2 list-group">
        {% for question in pagination.items %}
            <div class="list-group-item">
                {{ question.title }}
            </div>
        {% endfor %}
    </div>
    <div class="navigation text-center">
        {{ knp_pagination_render(pagination) }}
    </div>
{% endblock %}